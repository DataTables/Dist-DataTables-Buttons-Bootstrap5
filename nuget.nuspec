        <file src="js/buttons.bootstrap5.mjs" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/buttons.bootstrap5.css" target="content\Content"/>
        <file src="css/buttons.bootstrap5.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/buttons.bootstrap5.min.css" target="content\Content"/>
